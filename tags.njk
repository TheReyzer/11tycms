---
layout: layouts/base.njk
pagination:
  data: collections
  size: 1
  reverse: true
  alias: tag
  filter:
    - all
    - posts
---

<h1>a“{{ tag }}”</h1>
{% set postslist = collections[ tag ] %}

<ol>
{% for post in postslist %}
  <li><a href="{{ post.url }}">{{ post.data.title }}</a></li>
{% endfor %}
</ol>
